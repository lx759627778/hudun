/* pages/namelist/namelist.wxss */
.container{
  padding-bottom:30rpx;
}
.name-list{
  padding:0 15rpx;
}
.name-text{
  padding:0 15rpx;
  position: relative;
}
.name-text>view{
  display: block;
 
  font-size: 0rpx;
}

.name-text>view text{
  display: inline-block;
  margin-right: 10rpx;
  line-height: 90rpx;
  color:#2b2b2b;
  font-size: 32rpx;
}
.name-text>view text:first-child{
  width:128rpx;
}
.name-text image{
  width:16rpx;
  height:28rpx;
  position: absolute;
  right:15rpx;
  top:50%;
  margin-top:-14rpx;
}
.name-li>image{
  display: block;
  width:100%;
  height:8rpx;
}

.no-more{
  display: block;
  color:#8a8a8a;
  text-align: center;
  font-size: 36rpx;
  line-height: 1.5;
  margin-top:30rpx;
}