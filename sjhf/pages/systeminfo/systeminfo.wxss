page{
  background: #F2F6F9;
}
.model{
  width: 100%;
  height: 332rpx;
  background: #fff;
  position: relative;
  box-sizing: border-box;
  margin-bottom: 24rpx;
  padding-top:56rpx;
}
.model image.model-bg{
  position: absolute;
  top:0;
  left: 0;
  width:100%;
  z-index: 0;
  margin:0;
}
.model image{
  width: 140rpx;
  display: block;
  margin: 0 auto ;
  z-index: 1;
  position: relative;
}
.model text{
  line-height: 1;
  font-size: 28rpx;
  position: relative;
  z-index: 1;
  display: block;
  width:100%;
  text-align: center;
  color:#fff;
  margin-top:6rpx;
}
.sysinfo view{
  background: #fff;
  margin-bottom: 2rpx;
  height: 88rpx;
  padding: 0 32rpx;
  line-height: 88rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #2B2B2B;
}
.sysinfo view .result{
  color: #71bafa;
}
.btn{
  font-size: 32rpx;
  background: #5799F5;
  color: #fff;
  width: 668rpx;
  height: 82rpx;
  margin-top: 70rpx;
  border-radius: 4rpx;
}